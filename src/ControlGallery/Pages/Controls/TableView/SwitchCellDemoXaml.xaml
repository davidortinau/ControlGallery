<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/maui/global" 
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ControlGallery.Pages.Controls.TableView.SwitchCellDemoXaml"
Title="SwitchCell">
	<ContentPage.Content>
		<TableView>
			<TableRoot>
				<TableSection>
					<SwitchCell Text="On" On="true" />
					<SwitchCell Text="Off" On="false" />
				</TableSection>
			</TableRoot>
		</TableView>
	</ContentPage.Content>
</ContentPage>
